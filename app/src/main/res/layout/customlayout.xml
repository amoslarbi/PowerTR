<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent">

    <TextView
        android:id="@+id/appliance1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="37dp"
        android:layout_marginTop="4dp"
        android:textColor="#000000"
        android:textSize="17dp"
        android:text="Name of Appliance: " />

    <TextView
        android:id="@+id/number1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/appliance1"
        android:layout_below="@+id/appliance1"
        android:layout_marginTop="2dp"
        android:textColor="#000000"
        android:text="Socket Number: " />

    <TextView
        android:id="@+id/watts1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/number1"
        android:layout_below="@+id/number1"
        android:textColor="#000000"
        android:layout_marginTop="2dp"
        android:text="Watts Consumption: " />

    <TextView
        android:id="@+id/appliance"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/appliance1"
        android:layout_marginStart="22dp"
        android:layout_toEndOf="@+id/appliance1"
        android:textColor="#000000"
        android:textSize="17dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/number"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/appliance"
        android:layout_alignTop="@+id/number1"
        android:textColor="#000000"
        android:text="TextView" />

    <TextView
        android:id="@+id/watts"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/number"
        android:layout_alignTop="@+id/watts1"
        android:textColor="#000000"
        android:text="TextView" />

